      <Label fx:id="encounterLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="50.0" layoutY="23.0" prefHeight="52.0" prefWidth="501.0" styleClass="label-subtitle" text="See anything you like?" textAlignment="CENTER" />
      <Label alignment="TOP_LEFT" layoutX="297.0" layoutY="317.0" prefHeight="31.0" prefWidth="239.0" styleClass="label-small" text="What would you like to do?" textAlignment="CENTER" />
      <Button fx:id="fightButton" layoutX="399.0" layoutY="349.0" mnemonicParsing="false" onAction="#handleFightButtonAction" text="Fight" />
      <Button fx:id="fleeButton" layoutX="497.0" layoutY="349.0" mnemonicParsing="false" onAction="#handleFleeButtonAction" prefHeight="31.0" prefWidth="84.0" text="Flee" />
      <Button fx:id="otherButton" layoutX="297.0" layoutY="349.0" mnemonicParsing="false" onAction="#handleOtherButtonAction" text="Trade" />
   </children>
   <stylesheets>
      <URL value="@DarkTheme.css" />
   </stylesheets>
