<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="anchor-pane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="spacetrader.engine.ShipYardScreenController">
    <stylesheets>
        <URL value="@DarkTheme.css" />
    </stylesheets>
   <children>
      <ListView fx:id="shipList" layoutX="14.0" layoutY="64.0" prefHeight="283.0" prefWidth="200.0" />
      <Label layoutX="84.0" layoutY="27.0" styleClass="label-plain" text="Ships" />
      <Label fx:id="currentLabel" layoutX="250.0" layoutY="220.0" styleClass="label-plain" text="Current Ship" />
      <Label fx:id="selectedLabel" layoutX="250.0" layoutY="38.0" styleClass="label-plain" text="Selected Ship" />
      <Button fx:id="out" layoutX="33.0" layoutY="360.0" mnemonicParsing="false" onAction="#outButtonPressed" text="Out!" />
      <Button fx:id="buy" layoutX="120.0" layoutY="360.0" mnemonicParsing="false" onAction="#buyButtonPressed" text="Buy" />
   </children>
</AnchorPane>
